<?xml version="1.0"?>

<PropertyList>

<path>enginepanel.ac</path>

<animation>
  <object-name>N1L1</object-name>
  <object-name>N1L2</object-name>
  <object-name>N1L3</object-name>
  <object-name>N1R1</object-name>
  <object-name>N1R2</object-name>
  <object-name>N1R3</object-name>
  <emission>
    <red>0.5</red>
    <green>0.5</green>
    <blue>0.5</blue>
  </emission>
</animation>

<animation>
  <type>textranslate</type>
  <object-name>N1L1</object-name>
  <property>/engines/engine[0]/n1-actual</property>
  <factor>0.1</factor>
  <step>1</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>textranslate</type>
  <object-name>N1L2</object-name>
  <property>/engines/engine[0]/n1-actual</property>
  <factor>0.01</factor>
  <step>10</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>textranslate</type>
  <object-name>N1L3</object-name>
  <property>/engines/engine[0]/n1-actual</property>
  <factor>0.001</factor>
  <step>100</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

</PropertyList>

